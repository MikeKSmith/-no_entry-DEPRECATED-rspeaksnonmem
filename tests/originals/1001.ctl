$PROB 1001 phase1 1CMT
$INPUT C ID TIME SEQ=DROP EVID AMT DV
$DATA ../../data/derived/phase1.csv IGNORE=C
$SUBROUTINE ADVAN2 TRANS2
$PK
 CL=THETA(1)*EXP(ETA(1))
 V =THETA(2)*EXP(ETA(2))
 KA=THETA(3)*EXP(ETA(3))
 S2=V
 
$ERROR
 Y=F*EXP(ERR(1)) + ERR(2)
 IPRE=F

$THETA
(10)     ;CL
(10)      ;V
(0.2)     ;KA

$OMEGA 0.09 0.09 0.09
$SIGMA 0.09 0.1
$ESTIMATION MAXEVAL=9999 PRINT=5 NOABORT METHOD=1 INTER MSFO=./1001.msf
$COV PRINT=E
$TABLE NOPRINT FILE=./1001.tab ONEHEADER ID AMT TIME EVID PRED IPRE CWRES
$TABLE NOPRINT FILE=./1001par.tab ONEHEADER ID TIME CL V KA ETA1 ETA2 ETA3
